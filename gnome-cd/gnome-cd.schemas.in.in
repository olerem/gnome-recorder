<?xml version="1.0"?>
<gconfschemafile>
  <schemalist>
    <schema>
      <key>/schemas/apps/gnome-cd/device</key>
      <applyto>/apps/gnome-cd/device</applyto>
      <owner>Gnome-CD</owner>
      <type>string</type>
      <default>@default_cd_device@</default>
      <locale name="C">
	<short>Device path</short>
	<long>The path to the CD drive that the CD player should use to play CDs</long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/gnome-cd/on-start</key>
      <applyto>/apps/gnome-cd/on-start</applyto>
      <owner>Gnome-CD</owner>
      <type>int</type>
      <default>0</default>
      <locale name="C">
	<short>Action on start</short>
	<long>What action should the CD player perform on start up? 0 = Nothing, 1 = Start playing, 2 = Stop playing</long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/gnome-cd/close-on-start</key>
      <applyto>/apps/gnome-cd/close-on-start</applyto>
      <owner>Gnome-CD</owner>
      <type>bool</type>
      <default>FALSE</default>
      <locale name="C">
	<short>Shut the CD tray on start</short>
	<long>Should the CD player attempt to shut the CD drive on start up?</long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/gnome-cd/on-stop</key>
      <applyto>/apps/gnome-cd/on-stop</applyto>
      <owner>Gnome-CD</owner>
      <type>int</type>
      <default>1</default>
      <locale name="C">
	<short>Action on application exit</short>
	<long>What action should the CD player perform on exit? 0 = Nothing, 1 = Stop playing, 2 = Eject the CD, 3 = Attempt to close the tray</long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/gnome-cd/theme-name</key>
      <applyto>/apps/gnome-cd/theme-name</applyto>
      <owner>Gnome-CD</owner>
      <type>string</type>
      <default>media</default>
      <locale name="C">
	<short>CD player theme</short>
	<long>What theme should the CD player use for it's appearance</long>
      </locale>
    </schema>

  </schemalist>
</gconfschemafile>
